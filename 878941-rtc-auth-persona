+ * add tests to suite and run them
+ */
+
+let TESTS = [ 
+  test_import,
+  test_ready,
+  test_sign,
+  test_verify
+];
+
+TESTS.forEach(add_test);
+
+function run_test() {
+  run_next_test();
+}
